{
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",   
        "^@/components/(.*)$",
        "^@/screens/(.*)$",
        "^@/ui/(.*)$",
        "^@/hooks/(.*)$",
        "^@/shared/(.*)$",
        "^@/services/(.*)$",
        "^@/config/(.*)$",
        "^@/store/(.*)$",
        "^@/utils/(.*)$",
        "^@/assets/(.*)$",
        "^../(.*)",
        "^./(.*)",
        "(.scss)$"
    ],
   "importOrderSeparation": true, 
   "importOrderSortSpecifiers": true
}